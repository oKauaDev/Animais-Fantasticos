{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": 0
  }
}
